{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microapp.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Remote Attach",
            "type": "python",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 8888
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "${workspaceFolder}"
                }
            ]
        },
        {
            "name": "zlm_sender: Open UI",
            "type": "python",
            "request": "launch",
            "module": "zlm_sender",
            "args": [
                "-o"
            ],
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "ZlmUI",
            "type": "python",
            "request": "launch",
            "module": "zlm_ui",
            "args": [
                "368",
                "${workspaceFolder}\\layers.TXT"
            ],
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "ZLM_SETTINGS_PATH": "C:\\dev\\animtools\\GbxAnimLauncher\\Tools\\ZLayerManager",
                "ZLM_PRESET_PATH": "c:\\dev\\Cypress2\\ContentSource\\cypress2source\\Character\\__Shared\\Head\\ZBrush\\presets"
            }
        },
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
        },
        {
            "name": "UI",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\src\\zlm_ui\\__main__.py",
        }
    ]
}