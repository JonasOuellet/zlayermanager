{
    "files.exclude": {
        "src/zlm_env": true,
        "src/**/*.zsc": true,
        "**/__pycache__": true,
        "**/*.pyc": true,
        "src/build": true,
        "**/*.zsc": true,
        "docs_build": true,
    },
    "flake8.args": [
        "--max-line-length=130",
        "--ignore=E722"
    ],
    "restructuredtext.confPath": "${workspaceFolder}\\doc_src",
}